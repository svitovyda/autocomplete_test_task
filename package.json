  "scripts": {
    "build": "webpack",
    "build:prod": "ENV_NAME=production webpack",
    "clean:jest": "jest --clearCache",
    "clean:yarn": "rm -rf node_modules && yarn cache clean",
    "clean:files": "rm -rf *.tsbuildinfo output coverage build lib",
    "clean": "yarn clean:yarn && yarn clean:files",
    "rebuild": "yarn clean && yarn && yarn clean:jest",
    "compile": "npx tsc",
    "check:lint": "eslint --max-warnings=0 --ext .js,.ts,.tsx,.cjs,.mjs,.json . --resolve-plugins-relative-to .",
    "fix:lint": "eslint --ext .js,.ts,.tsx,.cjs,.mjs,.json --fix . --resolve-plugins-relative-to .",
    "check:prettier": "prettier --check './*.{js,ts,tsx,cjs,mjs,json}' 'src/**/*.{js,ts,tsx,cjs,mjs,json}' 'test/**/*.{js,ts,tsx,cjs,mjs,json}'",
    "fix:prettier": "prettier --write '*.{js,ts,tsx,cjs,mjs,json}' 'src/**/*.{js,ts,tsx,cjs,mjs,json}' 'test/**/*.{js,ts,tsx,cjs,mjs,json}'",
    "check": "yarn check:prettier && yarn check:lint",
    "fix": "yarn fix:prettier && yarn fix:lint",
    "start": "webpack serve --open",
    "test": "jest",
    "test:full": "yarn test --coverage"
  },
  "browserslist": [
    "chrome >= 116, not dead",
    "android >= 116, not dead",
    "firefox >= 116, not dead",
